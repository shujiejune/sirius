{
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@objects/*": ["src/objects/*"],
      "@assets/*": ["src/assets/*"],
      "@utils/*": ["src/utils/*"],
    },
  },
}
